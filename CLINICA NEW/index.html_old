body {        
    font-family: 'Montserrat', sans-serif;
}
@font-face{font-family:'exo';
  src:url("fonts/exo1.woff2");font-weight: 400;
}
@font-face{font-family:'exo';
src:url("fonts/exo.woff2");font-weight: 600;
}
.header {
  background: #153065;
}

.container{padding: 10px 10px 20px 10px;}
.container hr {
  margin-bottom: 20px;width: 80px;opacity: .4;border-top: .1px solid;display: block;
}

.white{color: #fff;}
.container h1 {
  font-family: sans-serif;
  font-size: 30px;    margin-bottom: 10px;margin-top: 0px;
  line-height: 1.1;    text-transform: uppercase;font-weight: 400;
}
.container p {
  font-size: 18px;margin-bottom: 40px;margin-top: 0px;
  line-height: 22px;font-style: italic;font-weight: 400;
  font-family: sans-serif;
}
.form-zone ul li {
  float: left;
  width: 50%;
  position: relative;
  background-clip: content-box;
  text-align: center;
}
.form-zone ul li select {
  text-transform: uppercase;
  color: #fff;
  width: 100%;
  padding: 5px 35px 5px 2px;
  font-size: 20px;
  border: none;outline: none;
  border-bottom: 1px solid #fff;
  height: 34px;
  background: transparent;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  font-size: 16px;
  margin-bottom: 40px;
}
option
{
  color: #000;
}
.form-zone ul li.weight {
  padding-left: 0px;padding-right: 20px;
}
.form-zone ul li.height {
  padding-left: 20px;padding-right: 0px;
}
.form-zone ul li.height select {
  float: right;
}
.form-zone ul li.height:before,.form-zone ul li.weight:before {
  right: 2px;
  top: 14px;
  content: " ";
  display: block;
  position: absolute;
  border-left: 6px solid transparent;
  border-right: 6px solid transparent;
  border-top: 6px solid #fff;
  color: #fff;
}
.form-zone ul li.weight:before {right: 24px}
.form-zone ul li.mobile-metrics {
  width: 100%;
  display: block;
  color: #fff;
}
.form-zone ul li.mobile-metrics ul li {
  display: block;
  width: 100%;
  margin-bottom: 10px;
}
.mobile-metrics ul li span {
  background: rgba(244,151,30,1);
  display: block;
  width: 23px;
  height: 23px;
  float: left;
  margin-right: 10px;
}
.mobile-metrics ul li p {
  margin: 0;
  text-align: initial;
  font-style: initial;
  font-size: 13px;
  line-height: 13px;
  display: block;
  float: left;
  width: calc(100% - 33px);
  padding: 5px 20px;background: rgba(244,151,30,0.3);
}
.mobile-metrics ul li.imc-normal span {
  background: rgba(84,184,71,1);
}
.mobile-metrics ul li.imc-normal p {
  background: rgba(84,184,71,0.3);
}
.mobile-metrics ul li.imc-medium span {
  background: rgba(177,167,53,1);
}
.mobile-metrics ul li.imc-medium p {
  background: rgba(177,167,53,0.3);
}
.mobile-metrics ul li.imc-fat1 span {
  background: rgba(244,151,30,1);
}
.mobile-metrics ul li.imc-fat1 p {
  background: rgba(244,151,30,0.3);
}
.mobile-metrics ul li.imc-fat2 span {
  background: rgba(210,111,42,1);
}
.mobile-metrics ul li.imc-fat2 p {
  background: rgba(210,111,42,0.3);
}
.mobile-metrics ul li.imc-superfat span {
  background: rgba(208,47,40,1);
}
.mobile-metrics ul li.imc-superfat p {
  background: rgba(208,47,40,0.3);
}
.form-zone ul li.full div a.adc-consent-button,
.form-zone ul li.full div input.adc-consent-button {
  border: 0;
  font-weight: bold;
  background: #e02049;
  width: 100%;
  display: block;
  text-align: center;
  text-decoration: none;
  color: #fff;
  font-size: 21px;
  padding: 20px 0 14px;
  border-bottom: 6px solid #bc193c;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  outline: 0;
  border-radius: 4px;
  font-family: sans-serif;
  height: 61px;
}
.form-zone ul li.full {
  margin-top: 0px;width: 100%;
}
.how-works .container{padding-top: 20px;}

.how-works .container span {
  text-align: center;
  font-family: sans-serif;
  font-weight: bold;
  text-transform: uppercase;
  margin-bottom: 0;font-size: 26px;    color: #2b393f;
  display: block;
}
.how-works .container hr {
  margin: 30px auto;
  opacity: .6;
  float: inherit;
}
.how-works .l1 amp-img,.how-works .l3 amp-img{width: 80px; margin:0 auto 5px;}
.how-works .l2 amp-img{width: 100px; margin:0 auto 5px;}
.how-works .l3 p{margin-bottom: 0px;}
.how-works p{font-style: normal;}
footer .container{    padding: 10px;}
footer .container span {
  width: 100%;
  display: block;
  text-align: center;
  font-weight: 300;
  font-size: 12px;    font-family: sans-serif;
}
.foo1{padding-bottom: 10px;}
footer .container span a{text-decoration: none;    color: #000;}
.form-zone{padding: 0 5px;}
.mobile-metrics ul li.imc-low.active p {
  background: rgba(244,151,30,1);
}
.mobile-metrics ul li.imc-normal.active p {
  background: rgba(84,184,71,1);
}
.mobile-metrics ul li.imc-superfat.active p {
  background: rgba(208,47,40,1);
}
.mobile-metrics ul li.imc-medium.active p {
  background: rgba(177,167,53,1);
}
.mobile-metrics ul li.imc-fat2.active p {
  background: rgba(210,111,42,1);
}
.mobile-metrics ul li.imc-fat1.active p {
  background: rgba(244,151,30,1);
}
.outline-fit,.metrics{
 display: none;
}
amp-lightbox {
  background: rgba(0,0,0,.8);text-align: center;
}
.adc-consent-modal-close {
  margin: 0 0 10px;
  color: #CBCBCB;
  font-size: 32px;
  cursor: pointer;
}
.adc-consent-modal-title {
  max-width: 440px;
  margin: 0 auto 20px;
  font-size: 25px;font-family: 'exo',sans-serif;
  font-weight: bold;
  text-align: center;line-height: normal;
  text-transform: uppercase;color: #474747;
}
.container amp-lightbox p,.inbox-checkbox {
  color: #474747;
  font-family: "exo", sans-serif;
  font-size: 16px;
  font-weight: normal;
  font-style: normal;
  line-height: normal;margin-bottom: 0;
}
.inbox-checkbox a{color:inherit;}
.lightbox{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
}
.adc-consent-modal-button {
  display: block;font-family: "exo", sans-serif;
  margin: 10px auto 0;
  padding: 10px 60px 12px;
  border-radius: 4px;
  background: #23C787;
  color: #FFFFFF;
  font-size: 26px;
  font-weight: bold;border: 0;
  text-align: center;margin-top: 40px;
}
.adc-consent-modal-button-cancel {
  padding: 12px 30px 14px;
  border: 2px solid #A3A3A3;
  background: none;display: inline-block;
  color: #A3A3A3;
  font-size: 20px;    margin: 10px auto 0;
}
#h-error,#w-error{
  font-size: 10px;
  color: red;
  margin: 0;
  position: absolute;
  top: 50%;
}
.light1{
  position: relative;
  width: 100%;
  max-width: 785px;
  padding: 20px 22px 50px;
  border-radius: 4px;
  background-color: #FFFFFF;
  text-align: center;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.33);
}
#consent-ui {
  background: rgb(255, 255, 255);
  padding: 15px 15px;border-top: 1px solid #ccc;
}
#consent-ui h3{
  font-family: sans-serif;
  font-size: 15px;
  margin: 0;
}
#consent-ui div{
  font-size: 13px;
  font-weight: 400;
  font-family: sans-serif;
  padding-top: 10px;    text-align: justify;
  color: #474747;
}
#consent-ui button{
  display: block;
  margin: 10px auto 0;
  padding: 8px 0px 8px;
  border-radius: 4px;
  background: #00bce3;
  color: #FFFFFF;
  font-size: 15px;
  text-align: center;
  font-family: sans-serif;
  letter-spacing: 0;
  outline: 0;
  border: 0;width: 180px;
  box-shadow: 1px 1px 1px #ccc;
}
#consent-ui span{
  background: url(images/checked.svg) no-repeat;
  background-size: contain;
  width: 15px;height: 15px;float: left;margin-left: 10px;
}
#consent-ui p a{color: #474747;font-size: 11px;text-decoration: none;font-family: sans-serif;}
.backtop{
  background-position: cover;
}
.overimage{
  position: relative;
  text-align: center;
}
.centered {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.white{
  color: #fff;
}
.social{

}
.social li{
  display: inline-block;
}
.social li a{
  color: #ffffff;
  display: block;
  position: relative;
  width: 41px;
  height: 61px;
  line-height: 52px;
  text-align: center;
  font-size: 30px;
  text-decoration: none;
  -webkit-transition: all 0.2s ease;
  -moz-transition: all 0.2s ease;
  -o-transition: all 0.2s ease;
  -ms-transition: all 0.2s ease;
  transition: all 0.2s ease;
}
